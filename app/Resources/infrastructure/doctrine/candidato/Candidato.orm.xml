<?xml version="1.0" encoding="utf-8"?><doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">    <entity repository-class="Infrastructure\Persistence\Doctrine\CandidatoRepository"            name="Domain\Model\Candidato\Candidato"            table="tb_candidato">        <id name="id" type="integer" column="id_candidato">            <generator strategy="AUTO"/>        </id>        <field name="nome" column="nome" type="string" />        <field name="cpf" column="cpf" type="string" />        <field name="email" column="email" type="string" />        <field name="celular" column="celular" type="string" />        <field name="telefone" column="telefone" type="string" />        <field name="link" column="link" type="string" nullable="true" />        <many-to-many field="habilidadesTecnicas" target-entity="Domain\Model\HabilidadeTecnica\HabilidadeTecnica">            <cascade>                <cascade-persist/>            </cascade>            <join-table name="tb_candidato_habilidade_tecnica">                <join-columns>                    <join-column name="id_candidato" referenced-column-name="id_candidato" />                </join-columns>                <inverse-join-columns>                    <join-column name="id_habilidade_tecnica" referenced-column-name="id_habilidade_tecnica" />                </inverse-join-columns>            </join-table>        </many-to-many>        <many-to-many field="experienciasProfissionais" target-entity="Domain\Model\ExperienciaProfissional\ExperienciaProfissional">            <cascade>                <cascade-persist/>            </cascade>            <join-table name="tb_candidato_exp_profissional">                <join-columns>                    <join-column name="id_candidato" referenced-column-name="id_candidato" />                </join-columns>                <inverse-join-columns>                    <join-column name="id_experiencia_profissional" referenced-column-name="id_experiencia_profissional" />                </inverse-join-columns>            </join-table>        </many-to-many>    </entity></doctrine-mapping>